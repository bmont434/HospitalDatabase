Create Statements
#1
CREATE TABLE COUNTRY
(COUNTRY_ID             NUMBER(9, 0) CONSTRAINT CTRY_ID_PK PRIMARY KEY,
COUNTRY_NAME           VARCHAR(30)
);
#2
CREATE TABLE ADDRESS 
   (	ADDRESS_ID NUMBER(9,0) CONSTRAINT ADD_ID_PK PRIMARY KEY,
	STREET VARCHAR2(25), 
	STATE CHAR(2), 
	CITY VARCHAR2(30), 
	ZIP_CODE NUMBER(5,0),
    COUNTRY_ID NUMBER(9, 0),
CONSTRAINT AD_CO_ID_FK FOREIGN KEY (COUNTRY_ID) REFERENCES COUNTRY(COUNTRY_ID));   

#3
CREATE TABLE EMPLOYEES
(EMPLOYEE_ID            NUMBER(9, 0) CONSTRAINT EMP_ID_PK PRIMARY KEY,
LAST_NAME               VARCHAR2(25),
FIRST_NAME              VARCHAR2(25),
EMPLOYEE_TITLE          VARCHAR2(30),
EXPERIENCE_LEVEL        VARCHAR2(25),
ADDRESS_ID              NUMBER(9, 0),
CONSTRAINT AD_ID_FK FOREIGN KEY (ADDRESS_ID) REFERENCES ADDRESS(ADDRESS_ID));
#3
CREATE TABLE PATIENT
(PATIENT_ID            NUMBER(9, 0) CONSTRAINT PAT_ID_PK PRIMARY KEY,
LAST_NAME               VARCHAR2(25),
FIRST_NAME              VARCHAR2(25),
INSURANCE               VARCHAR2(30),
PHONE_NUMBER            NUMBER(5, 0),
GENDER                  VARCHAR2(10),
AGE                     NUMBER(2, 0),
WEIGHT                  NUMBER(4, 0),
ADDRESS_ID              NUMBER(9, 0),
CONSTRAINT AD2_ID_FK FOREIGN KEY (ADDRESS_ID) REFERENCES ADDRESS(ADDRESS_ID));
#4
CREATE TABLE STAY
(STAY_ID   	NUMBER(9, 0) CONSTRAINT STY_ID_PK PRIMARY KEY,
START_DATE              DATE,
END_DATE                DATE,
PATIENT_ID              NUMBER(9, 0),
CONSTRAINT PTN_ID_FK FOREIGN KEY (PATIENT_ID) REFERENCES PATIENT(PATIENT_ID));
#5
CREATE TABLE ROOM
(ROOM_NO                NUMBER(9, 0) CONSTRAINT RM_NO_PK PRIMARY KEY,
FLOOR                   NUMBER(1, 0),
SECTION                 VARCHAR2(5),
STAY_ID                 NUMBER(9, 0),
CONSTRAINT ST_ID_FK FOREIGN KEY (STAY_ID) REFERENCES STAY(STAY_ID));
#6
CREATE TABLE PT_DIAGNOSIS
(DIAGNOSIS_ID           NUMBER(9, 0) CONSTRAINT DIA_ID_PK PRIMARY KEY,
DETAILS                 VARCHAR2(100),
DIAG_DATE               DATE,
PATIENT_ID              NUMBER(9, 0),
EMPLOYEE_ID              NUMBER(9, 0),
CONSTRAINT PATI_ID_FK FOREIGN KEY (PATIENT_ID) REFERENCES PATIENT(PATIENT_ID),
CONSTRAINT EMPP_ID_FK FOREIGN KEY (EMPLOYEE_ID) REFERENCES EMPLOYEES(EMPLOYEE_ID));
#7
CREATE TABLE RECOMMENDATION
(REC_ID                 NUMBER(9, 0) CONSTRAINT REC_ID_PK PRIMARY KEY,
REC_TYPE                VARCHAR2(25),
DETAILS                 VARCHAR(100),
DIAGNOSIS_ID            NUMBER(9,0),
CONSTRAINT DIIA_ID_FK FOREIGN KEY (DIAGNOSIS_ID) REFERENCES PT_DIAGNOSIS(DIAGNOSIS_ID));



